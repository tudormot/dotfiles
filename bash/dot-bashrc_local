# This is a bash config file, containing local settings

alias cssh='ssh tudorm.c.googlers.com'
alias crw='rw --enable_x_forwarding --check_remaining --reconnect_loop --reconnect_loop_duration_s 86400 --tmux --tmux_session=work_default tudorm.c.googlers.com'
alias grw='rw --enable_x_forwarding --check_remaining --reconnect_loop --reconnect_loop_duration_s 86400 --tmux --tmux_session=gabp_demo tmot-gpu-cloudtop.c.googlers.com'

alias gemini-cli='/google/bin/releases/gemini-cli/tools/gemini'


export PYENV_ROOT="$HOME/.pyenv"
[[ -d $PYENV_ROOT/bin ]] && export PATH="$PYENV_ROOT/bin:$PATH"
eval "$(pyenv init - bash)"

